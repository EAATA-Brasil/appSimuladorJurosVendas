{
  "expo": {
    "name": "Simulador EAATA",
    "slug": "simuladorJurosVendas",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "simuladorjurosvendas",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "updates": {
      "url": "https://u.expo.dev/4621528f-21be-49b5-a6a7-a661e54f2e29"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eaata.simuladorJurosVendas",
      "infoPlist": {
        "NSPhotoLibraryAddUsageDescription": "O app salva relatórios em PDF na sua galeria.",
        "NSPhotoLibraryUsageDescription": "Permita acesso para salvar PDFs.",
        "NSDocumentsFolderUsageDescription": "O app precisa acessar sua pasta de Documentos para armazenar arquivos.",
        "NSCameraUsageDescription": "Permita acesso à câmera para capturar documentos.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "entitlements": {
        "com.apple.developer.icloud-services": [
          "CloudDocuments"
        ],
        "com.apple.developer.icloud-container-identifiers": [
          "iCloud.com.eaata.simuladorJurosVendas"
        ]
      }
    },
    "android": {
      "package": "com.eaata.simuladorJurosVendas",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icon.png",
        "backgroundColor": "#009de0"
      },
      "permissions": [
        "INTERNET",
        "ACCESS_NETWORK_STATE",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/icon.png",
      "build": {
        "publicPath": "/simulador/"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#009de0"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true,
            "networkSecurityConfig": true,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "15.1"
          }
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production",
          "iCloudContainerIdentifier": "iCloud.com.eaata.simuladorJurosVendas"
        }
      ],
      "expo-web-browser",
      "expo-asset"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "4621528f-21be-49b5-a6a7-a661e54f2e29"
      },
      "apiUrl": "http://eaatasimulator/api"
    },
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "androidStatusBar": {
      "backgroundColor": "#009de0"
    }
  }
}
